<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
        <key>RunAtLoad</key>
        <true/>
        <key>KeepAlive</key>
        <true/>
        <key>Label</key>
        <string>de.kokodev.SnippetSync</string>
        <key>StandardOutPath</key>
        <string>/path/to/snippetsync.log</string>
        <key>ProgramArguments</key>
        <array>
            <string>/path/to/SnippetSync</string>
            <string>-o</string>
            <string>/path/to/sync/folder</string>
            <string>-c</string>
            <string>-i</string>
            <string>-s</string>
            <string>-f</string>
        </array>
</dict>
</plist>
